{
  "compilerOptions": {
    "outDir": "./dist",
    "strict": false,
    "noImplicitAny": false,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "lib": ["es2015", "dom"],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"],

}